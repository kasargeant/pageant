{
  "name": "pageant",
  "version": "0.1.1",
  "description": "A pretty-printing console module extracted from Warhorse.",
  "keywords": [
    "javascript",
    "console",
    "JSON"
  ],
  "author": "Kyle Alexis Sargeant",
  "email": "kasargeant@gmail.com",
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/kasargeant/pageant.git"
  },
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "engines": {
    "node": "~6.10.0",
    "npm": "~3.10.10"
  },
  "scripts": {
    "test": "jest --config ./conf/jest.json",
    "test-cover": "jest --config ./conf/jest.json --coverage",
    "test-update": "jest --config ./conf/jest.json --updateSnapshot"
  },
  "jest-html-reporter": {
    "pageTitle": "Test Results",
    "outputPath": "docs/tests/index.html",
    "includeFailureMsg": false
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "jest": "^20.0.4",
    "jest-html-reporter": "^0.1.0",
    "minami": "^1.2.3"
  },
  "dependencies": {
    "easy-table": "^1.1.0",
    "jest-html-reporter": "^0.1.2",
    "linewrap": "^0.2.1",
    "tinter": "^0.4.5"
  }
}
